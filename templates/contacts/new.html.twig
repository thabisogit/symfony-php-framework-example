{% extends 'base.html.twig' %}
{% block title %}New Contact{% endblock %}
 	
 	{% block body %}
 	{{ form_start(form) }}
 	{{ form_row(form.title, {'label':'Contact','attr':{'class':'form-control','placeholder': 'Contact Title'}}) }}
 	{{ form_row(form.Manager, {'label':'Manager','attr':{'class':'form-control','placeholder': 'Contact Manager Email Address'}}) }}
 	{{ form_row(form.body, {'label':'Note','attr':{'class':'form-control','placeholder': 'Special Note...'}}) }}
 	<br>
 	<div id="html_element" data-callback="recaptchaCallback" class="g-recaptcha" data-sitekey="6LcI05kUAAAAAEMiJjj4tF-_TOqv3pBcqfPyCYZM"></div>
      <br/>
 	{{ form_row(form.save) }}
	 <br>
	 <input value="Page Scrap Insert" type="button"  onclick="scrap_content()">

 	
      
 	{{ form_end(form) }}

 	{% endblock %}

 	{% block javascripts %}
	<script src="/js/main.js"></script>
	{% endblock %}
	 